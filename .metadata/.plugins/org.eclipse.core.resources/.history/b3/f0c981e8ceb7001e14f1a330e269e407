package Assignment2;
import Assignment2.*;
import java.util.*;


import java.util.ArrayList;

public class StudentManager {
	
	private ArrayList<Student> studentList = new ArrayList<>() ;
	void addNewStudent(String name, int age, double grade) 
	{
		try 
		{
			Student s = new Student(name,age,grade);                            // illegal argument exception   IllegalStateException
			studentList.add(s);
			System.out.println();
		}
		catch(ArithmeticException e)
		{
			System.out.println(e.getMessage());
		}
		
	}
	void sortStudentsByName()
	{
		try 
		{
			System.out.println();
		}
		catch(ArithmeticException e)
		{
			System.out.println(e.getMessage());
		}
	}
	public static void main(String[] args)
	{
		Student s1 = new Student("Aditi",35,8.02);
		Student s2 = new Student("Ati",31,5.02);
		Student s3 = new Student("Akkti",33,8.02);
		Student s4 = new Student("Adijhkti",31,3.02);
		Student s5 = new Student("Aditiftf",31,4.02);
		
		
		
		
		
	}
}
