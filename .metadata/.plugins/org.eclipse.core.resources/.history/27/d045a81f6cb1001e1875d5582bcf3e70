package Jan12;

public class DiamondStar {

	public static void main(String[] args)
	{
		int N = 5;
		for(int row=1; row<=N; row++)
		{
			for(int sp=1; sp<=(N-(row+1)) ; sp++)
				System.out.print("4");
			for(int col=1;col<=(N-(N-1));col++)
				System.out.print("*");
			System.out.println(" ");
			
		}
	}
}
