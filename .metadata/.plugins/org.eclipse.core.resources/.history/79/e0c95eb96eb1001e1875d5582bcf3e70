package Jan12;

public class DiamondStar {

	public static void main(String[] args)
	{
		int N = 5;
		int numStars = 1;
		for(int row=1; row<=N; row++)
		{
			for(int sp=0; sp<=(N-(row+1)) ; sp++)
				System.out.print(" ");
			for(int col=1;col<=numStars;col++)
				System.out.print("*");
			System.out.println(" ");
			numStars+=2;		
		}
		for(int row=1; row<N; row++)
		{
			for(int sp=1; sp<=row; sp++)
				System.out.print(" ");
			for(int col=1; col<=N; col++)
				System.out.print("5");
			System.out.println("");
		}
	}
}
