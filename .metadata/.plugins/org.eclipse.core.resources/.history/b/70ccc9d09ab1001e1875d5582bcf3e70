package Jan12;

/**
 * 
 */
public class DiamondStarTest {
	public static void main(String[] args)
	{
		int N =5;
		int starNum=1;
		for(int i=0; i<N;i++)
		{
//			for(int sp=0;sp<N;sp++) {
//				System.out.print(" ");
//			}
			for(int j=starNum;j<=starNum;j++) {
				System.out.print("*");
			}
			System.out.println("");
			starNum+=2;
			
		}
			
	}
		
}
