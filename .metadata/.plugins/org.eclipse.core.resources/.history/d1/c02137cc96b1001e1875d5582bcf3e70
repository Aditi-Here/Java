package Jan12;

/**
 * 
 */
public class DiamondStarTest {
	public static void main(String[] args)
	{
		int N =5;
		for(int i=0; i<=N;i++) {
			for(int sp=0;sp<i;sp++)
			{
				System.out.print(" ");
			}
			for(int j=1;j<=N;j++) {
				System.out.print("*");
			}
			System.out.println("");
		}
			
	}
		
}
