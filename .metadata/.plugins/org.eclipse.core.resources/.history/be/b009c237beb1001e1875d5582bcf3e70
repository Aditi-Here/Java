package Jan12;

/**
 * 
 */
public class DiamondStarTest {
	public static void main(String[] args)
	{
		int N =5;
		int starNum=1;
		for(int i=0; i<N;i++)
		{
			for(int sp=0;sp<(N-i);sp++) {
				System.out.print(" ");
			}
			for(int j=0;j<starNum;j++) {
				System.out.print("*");
			}
			System.out.println("");
			starNum+=2;
		}
		for(int i=(N-1);i>1;i--)
		{
			
			for(int sp=0;sp<(N-i);sp++)
				System.out.print(" ");
			for(int j=0; j<N;j++)
				System.out.println("*");
			System.out.println("");
		}
			
		
			
	}
		
}
